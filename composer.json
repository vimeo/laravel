{
    "name": "vimeo/laravel",
    "description": "A Vimeo bridge for Laravel",
    "license": "Apache-2.0",
    "keywords": ["laravel", "vimeo", "video"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.5.*",
        "illuminate/support": "5.5.*",
        "graham-campbell/manager": "^3.0",
        "vimeo/vimeo-api": "^2.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Vimeo\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vimeo\\Tests\\Laravel\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Vimeo\\Laravel\\VimeoServiceProvider"
            ],
            "aliases": {
                "Vimeo": "Vimeo\\Laravel\\Facades\\Vimeo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
