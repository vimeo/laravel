{
	"name": "vinkla/vimeo",
	"description": "Laravel wrapper for the official Vimeo API.",
	"license": "MIT",
	"keywords": ["laravel", "vimeo", "video"],
	"authors": [
		{
			"name": "Vincent Klaiber",
			"email": "hello@vinkla.com"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"illuminate/contracts": "5.0.*|5.1.*",
		"illuminate/support": "5.0.*|5.1.*",
		"graham-campbell/manager": "~2.1",
		"vimeo/vimeo-api": "~1.0"
	},
	"require-dev": {
		"graham-campbell/testbench": "~2.1"
	},
	"autoload": {
		"psr-4": {
			"Vinkla\\Vimeo\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Vinkla\\Tests\\Vimeo\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
